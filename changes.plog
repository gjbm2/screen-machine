diff --git a/src/components/TestUpload.tsx b/src/components/TestUpload.tsx
new file mode 100644
index 00000000..18c60e06
--- /dev/null
+++ b/src/components/TestUpload.tsx
@@ -0,0 +1,247 @@
+import React, { useState, useRef, useEffect } from 'react';
+import { 
+  putPhoto, 
+  takePhotoFromCache, 
+  saveUiSnapshot, 
+  loadUiSnapshot, 
+  clearUiSnapshot,
+  clearPhotoCache,
+  type UiSnapshot 
+} from '../utils/photoCache';
+
+const TestUpload: React.FC = () => {
+  const [photoDataUri, setPhotoDataUri] = useState<string>('');
+  const [fileName, setFileName] = useState<string>('');
+  const [fileSize, setFileSize] = useState<number>(0);
+  const [error, setError] = useState<string>('');
+  const [isLoading, setIsLoading] = useState<boolean>(false);
+  const [debugMessage, setDebugMessage] = useState<string>('');
+  
+  const cameraInputRef = useRef<HTMLInputElement>(null);
+
+  const addDebugMessage = (message: string) => {
+    setDebugMessage(prev => prev + '\n' + new Date().toLocaleTimeString() + ': ' + message);
+  };
+
+  // On mount: check if we have a photo in DB from previous session
+  useEffect(() => {
+    const checkForCachedPhoto = async () => {
+      setIsLoading(true);
+      addDebugMessage('Checking for cached photos...');
+      try {
+        const cachedPhoto = await takePhotoFromCache();
+        if (cachedPhoto) {
+          addDebugMessage('Found cached photo, displaying...');
+          displayPhoto(cachedPhoto);
+        } else {
+          addDebugMessage('No cached photo found');
+        }
+      } catch (error) {
+        addDebugMessage(`Error checking cache: ${error.message}`);
+      } finally {
+        setIsLoading(false);
+      }
+    };
+
+    checkForCachedPhoto();
+  }, []);
+
+  const displayPhoto = (file: File | Blob) => {
+    try {
+      addDebugMessage('Starting file reader for display...');
+      const reader = new FileReader();
+      
+      reader.onload = (e) => {
+        addDebugMessage('File reader completed');
+        const dataUri = e.target?.result as string;
+        setPhotoDataUri(dataUri);
+        setFileName(`photo-${Date.now()}.jpg`);
+        setFileSize(file.size);
+        addDebugMessage('Photo displayed successfully');
+      };
+      
+      reader.onerror = (e) => {
+        addDebugMessage('File reader error occurred');
+        setError('Failed to read photo file');
+      };
+      
+      reader.readAsDataURL(file);
+    } catch (error) {
+      addDebugMessage(`Error in displayPhoto: ${error.message}`);
+      setError(`Failed to display photo: ${error.message}`);
+    }
+  };
+
+  const handleCameraInput = async (event: React.ChangeEvent<HTMLInputElement>) => {
+    const file = event.target.files?.[0];
+    if (!file) {
+      addDebugMessage('No file selected');
+      return;
+    }
+
+    addDebugMessage(`Photo taken: ${file.name}, ${file.size} bytes, ${file.type}`);
+
+    try {
+      // 1. IMMEDIATELY save to DB FIRST (in case of browser refresh)
+      addDebugMessage('Saving to IndexedDB FIRST...');
+      await putPhoto(file);
+      addDebugMessage('Photo saved to DB successfully');
+
+      // 2. Only AFTER DB save, display the photo
+      addDebugMessage('Now displaying photo...');
+      displayPhoto(file);
+
+      // 3. Clear the input
+      if (cameraInputRef.current) {
+        cameraInputRef.current.value = '';
+      }
+
+    } catch (error) {
+      addDebugMessage(`Error processing photo: ${error.message}`);
+      setError(`Failed to process photo: ${error.message}`);
+    }
+  };
+
+  const takePhoto = () => {
+    addDebugMessage('Take photo button clicked');
+    // Save state before opening camera
+    saveUiSnapshot({ timestamp: Date.now() });
+    addDebugMessage('UI state saved to session storage');
+    
+    // Open camera
+    if (cameraInputRef.current) {
+      cameraInputRef.current.click();
+      addDebugMessage('Camera input clicked');
+    }
+  };
+
+  const retakePhoto = async () => {
+    addDebugMessage('Retake photo clicked');
+    setPhotoDataUri('');
+    setFileName('');
+    setFileSize(0);
+    setError('');
+    await clearPhotoCache();
+    addDebugMessage('Photo cache cleared');
+  };
+
+  const clearDebug = () => {
+    setDebugMessage('');
+  };
+
+  const formatFileSize = (bytes: number): string => {
+    if (bytes === 0) return '0 Bytes';
+    const k = 1024;
+    const sizes = ['Bytes', 'KB', 'MB', 'GB'];
+    const i = Math.floor(Math.log(bytes) / Math.log(k));
+    return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];
+  };
+
+  if (isLoading) {
+    return (
+      <div className="max-w-md mx-auto mt-8 p-6 bg-white rounded-lg shadow-lg">
+        <div className="flex items-center justify-center py-8">
+          <div className="animate-spin rounded-full h-8 w-8 border-b-2 border-blue-600"></div>
+          <span className="ml-3 text-gray-600">Loading...</span>
+        </div>
+      </div>
+    );
+  }
+
+  return (
+    <div className="max-w-md mx-auto mt-8 p-6 bg-white rounded-lg shadow-lg">
+      <h2 className="text-2xl font-bold mb-6 text-center text-gray-800">
+        Take Photo
+      </h2>
+      
+      {/* Debug Messages */}
+      {debugMessage && (
+        <div className="mb-4 p-3 bg-gray-100 border border-gray-300 rounded-lg">
+          <div className="flex justify-between items-center mb-2">
+            <h3 className="text-sm font-semibold text-gray-700">Debug Log:</h3>
+            <button
+              onClick={clearDebug}
+              className="text-xs bg-gray-500 text-white px-2 py-1 rounded"
+            >
+              Clear
+            </button>
+          </div>
+          <pre className="text-xs text-gray-600 whitespace-pre-wrap max-h-32 overflow-y-auto">
+            {debugMessage}
+          </pre>
+        </div>
+      )}
+      
+      {/* Hidden camera input */}
+      <input
+        ref={cameraInputRef}
+        type="file"
+        accept="image/*"
+        capture="environment"
+        onChange={handleCameraInput}
+        className="hidden"
+      />
+      
+      {!photoDataUri ? (
+        <div className="space-y-4">
+          <button
+            onClick={takePhoto}
+            className="w-full bg-blue-600 hover:bg-blue-700 text-white font-medium py-4 px-6 rounded-lg transition-colors flex items-center justify-center space-x-3 text-lg"
+          >
+            <svg className="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
+              <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M3 9a2 2 0 012-2h.93a2 2 0 001.664-.89l.812-1.22A2 2 0 0110.07 4h3.86a2 2 0 011.664.89l.812 1.22A2 2 0 0018.07 7H19a2 2 0 012 2v9a2 2 0 01-2 2H5a2 2 0 01-2-2V9z" />
+              <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M15 13a3 3 0 11-6 0 3 3 0 016 0z" />
+            </svg>
+            <span>Take Photo</span>
+          </button>
+          
+          <div className="text-sm text-gray-600 bg-gray-50 p-3 rounded-lg">
+            <p>📱 This will open your camera app. Your photo will be saved automatically.</p>
+          </div>
+          
+          {error && (
+            <div className="p-3 bg-red-100 border border-red-400 text-red-700 rounded-lg">
+              {error}
+            </div>
+          )}
+        </div>
+      ) : (
+        <div className="space-y-4">
+          {/* Photo Preview */}
+          <div className="border border-gray-300 rounded-lg p-4">
+            <img
+              src={photoDataUri}
+              alt="Captured photo"
+              className="w-full h-64 object-cover rounded-lg mb-3"
+            />
+            <div className="text-sm text-gray-600 space-y-1">
+              <p><strong>File:</strong> {fileName}</p>
+              <p><strong>Size:</strong> {formatFileSize(fileSize)}</p>
+            </div>
+          </div>
+          
+          {/* Action Buttons */}
+          <div className="flex space-x-3">
+            <button
+              onClick={retakePhoto}
+              className="flex-1 bg-gray-600 hover:bg-gray-700 text-white font-medium py-2 px-4 rounded-lg transition-colors"
+            >
+              Take Another
+            </button>
+            <button
+              onClick={() => {
+                console.log('Photo ready for upload:', photoDataUri);
+                alert('Photo would be uploaded here!');
+              }}
+              className="flex-1 bg-green-600 hover:bg-green-700 text-white font-medium py-2 px-4 rounded-lg transition-colors"
+            >
+              Upload
+            </button>
+          </div>
+        </div>
+      )}
+    </div>
+  );
+};
+
+export default TestUpload; 
