+// Create a thumbnail for preview using Web Worker APIs
+function createThumbnail(file: File): Promise<Blob> {
+  return new Promise(async (resolve, reject) => {
+    try {
+      // Use createImageBitmap which is available in Web Workers
+      const bitmap = await createImageBitmap(file);
+      
+      // Create an OffscreenCanvas (available in Web Workers)
+      const canvas = new OffscreenCanvas(bitmap.width, bitmap.height);
+      const ctx = canvas.getContext('2d');
+      
+      if (!ctx) {
+        reject(new Error('Failed to get canvas context'));
+        return;
+      }
+      
+      // Calculate thumbnail dimensions (max 300x300 for preview)
+      const maxSize = 300;
+      let { width, height } = bitmap;
+      
+      if (width > height) {
+        if (width > maxSize) {
+          height = (height * maxSize) / width;
+          width = maxSize;
+        }
+      } else {
+        if (height > maxSize) {
+          width = (width * maxSize) / height;
+          height = maxSize;
+        }
+      }
+      
+      // Create a new canvas with the target size
+      const thumbnailCanvas = new OffscreenCanvas(width, height);
+      const thumbnailCtx = thumbnailCanvas.getContext('2d');
+      
+      if (!thumbnailCtx) {
+        reject(new Error('Failed to get thumbnail canvas context'));
+        return;
+      }
+      
+      // Draw the resized image
+      thumbnailCtx.drawImage(bitmap, 0, 0, width, height);
+      
+      // Convert to blob
+      const blob = await thumbnailCanvas.convertToBlob({ type: 'image/jpeg', quality: 0.8 });
+      
+      // Clean up
+      bitmap.close();
+      
+      resolve(blob);
+    } catch (error) {
+      reject(error);
+    }
+  });
+}
+
+self.onmessage = async (event) => {
+  try {
+    const { type, file } = event.data;
+    
+    if (type === 'process') {
+      // Keep original file unchanged for upload
+      const originalFile = file;
+      
+      // Create a thumbnail for preview
+      const thumbnailBlob = await createThumbnail(file);
+      
+      self.postMessage({
+        type: 'processed',
+        originalFile: originalFile,
+        thumbnailBlob: thumbnailBlob
+      });
+    }
+  } catch (error) {
+    self.postMessage({
+      type: 'error',
+      error: error instanceof Error ? error.message : 'Unknown error'
+    });
+  }
+}; 
